{
	"name": "vbb-shapes",
	"description": "Shapes of VBB lines.",
	"version": "2.15.0",
	"main": "index.js",
	"files": [
		"index.js",
		"all.js",
		"compress.js",
		"uncompress.js",
		"ids.json",
		"data/*.json"
	],
	"keywords": [
		"vbb",
		"berlin",
		"brandenburg",
		"public transport",
		"open data",
		"geojson"
	],
	"author": "Jannis R <mail@jannisr.de>",
	"homepage": "https://github.com/derhuerst/vbb-shapes",
	"repository": "derhuerst/vbb-shapes",
	"bugs": "https://github.com/derhuerst/vbb-shapes/issues",
	"license": "ISC",
	"engines": {
		"node": ">=6"
	},
	"dependencies": {
		"from2": "^2.3.0"
	},
	"devDependencies": {
		"csv-parser": "^1.11.0",
		"shorthash": "^0.0.2",
		"strip-bom-stream": "^3.0.0",
		"json-stable-stringify": "^1.0.1",
		"queue": "^4.2.1"
	},
	"scripts": {
		"download": "curl -L --compressed https://vbb-gtfs.jannisr.de/latest/shapes.txt > build/shapes.txt",
		"convert": "find data -name '*.json' -print0 | xargs -0 rm; node build/index.js",
		"cleanup": "rm build/*.txt",
		"build": "npm run download && npm run convert && npm run cleanup",
		"test": "node test.js",
		"prepublishOnly": "npm run build && npm test"
	}
}
